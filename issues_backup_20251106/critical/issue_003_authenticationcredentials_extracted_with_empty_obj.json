        "location": "chargebee_data.json:10-16",
        "extracted": "\"authentication\": [\n  {},\n  {\n    \"name\": \"chargebeeApi\"\n  },\n  {}\n]",
        "source": "credentials: [\n  {\n    name: 'chargebeeApi',\n    required: true,\n  },\n]",
