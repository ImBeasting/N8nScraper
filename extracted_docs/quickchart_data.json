          ]
        }
      ],
      "typeOptions": {
        "multipleValues": true
      },
      "displayOptions": {
        "show": {
          "labelsMode": [
            "manually"
          ]
        }
      }
    },
    {
      "displayName": "Labels Array",
      "name": "labelsArray",
      "type": "string",
      "description": "The array of labels to be used in the chart",
      "placeholder": "e.g. [\"Berlin\", \"Paris\", \"Rome\", \"New York\"]",
      "default": "",
      "required": true,
      "displayOptions": {
        "show": {
          "labelsMode": [
            "array"
          ]
        }
      }
    },
    {
